<!--pages/camera/camera.wxml-->
<camera device-position="{{ cameraDirection }}" resolution="high" flash="off" binderror="cameraPermissionFailed" style="width: 100%; height: 500px;"></camera>
<view class="button-wrapper">
  <!-- <button type="primary" bindtap="bindViewRecord">录像</button> -->
  <text class="hint">{{hint}}</text>
  <button type="primary" bindtap="bindViewTakePhoto">拍摄</button>
  <button type="primary" bindtap="bindViewReverse">翻转</button>
  <button type="primary" bindtap="bindViewUpload">上传检测</button>
</view>